{
  "name": "userid",
  "version": "1.2.3",
  "description": "Lookup uid and gid information in node.js. Uses native POSIX bindings, not processes.",
  "main": "lib/userid.js",
  "types": "lib/userid.d.ts",
  "files": [
    "lib/userid.js",
    "lib/userid.d.ts",
    "src/userid.hh",
    "src/userid.cc",
    "src/Ids.cc",
    "src/Gid.cc",
    "src/Gids.cc",
    "src/UserName.cc",
    "src/GroupName.cc",
    "binding.gyp"
  ],
  "os": [
    "!win32"
  ],
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
