  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^4.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "^16.0.0",
    "cspell": "^5.6.6",
    "mocha": "^9.0.1",
    "node-gyp": "^8.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "should": "^13.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
